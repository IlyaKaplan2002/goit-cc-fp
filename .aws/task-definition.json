        },
        {
          "name": "APP_AUTH_ACCESS_SECRET",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:051859106932:secret:goit-cc-fp-secrets-IyOnaEAPP_AUTH_ACCESS_SECRET::"
