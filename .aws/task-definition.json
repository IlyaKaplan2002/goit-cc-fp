        },
        {
          "name": "APP_ENCRYPTION_KEY",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:051859106932:secret:goit-cc-fp-secrets-IyOnaEAPP_ENCRYPTION_KEY::"
