{
  "containerDefinitions": [
    {
      "name": "goit-cc-fp-container",
      "cpu": 0,
      "portMappings": [
        {
          "name": "goit-cc-fp-container-3030-tcp",
          "containerPort": 3030,
          "hostPort": 3030,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "APP_ENV",
          "value": "prod"
        },
        {
          "name": "TZ",
          "value": "UTC"
        },
        {
          "name": "APP_CORS_WHITE_LIST",
          "value": "http://127.0.0.1:3000"
        },
        {
          "name": "APP_FEATURE_SWAGGER",
          "value": "true"
        },
        {
          "name": "APP_FEATURE_SENTRY",
          "value": "false"
        },
        {
          "name": "APP_PORT",
          "value": "3030"
        },
        {
          "name": "APP_URL",
          "value": "goit-cc-fp-alb-1125089198.us-east-2.elb.amazonaws.com"
        }
      ],
      "environmentFiles": [],
      "mountPoints": [],
      "secrets": [
        {
          "name": "APP_DB_PORT",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:051859106932:secret:goit-cc-fp-secrets-IyOnaEAPP_DB_PORT::"
        },
        {
